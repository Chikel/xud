{
  "name": "xud",
  "version": "1.0.0-alpha.1",
  "description": "Exchange Union Daemon",
  "main": "lib/Xud.js",
  "bin": {
    "xud": "./bin/xud",
    "xucli": "./bin/xucli"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha dist/test/unit --reporter spec",
    "test:integration": "mocha dist/test/integration --reporter spec",
    "start": "npm run compile && node dist/lib/Xud.js",
    "dev": "npm run compile && nodemon lib/Xud.js",
    "compile": "rm -rf ./dist && tsc",
    "lint": "./node_modules/.bin/eslint . ./**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExchangeUnion/xud.git"
  },
  "keywords": [
    "Exchange Union",
    "XU",
    "xud",
    "bitcoin",
    "litecoin",
    "ethereum",
    "cryptocurrency",
    "lightning",
    "lightning network",
    "raiden",
    "DEX",
    "decentralized exchange",
    "payment channels",
    "atomic swaps",
    "cross-chain"
  ],
  "author": "Daniel McNally",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ExchangeUnion/xud/issues"
  },
  "homepage": "https://github.com/ExchangeUnion/xud#readme",
  "dependencies": {
    "@types/protobufjs": "^6.0.0",
    "command-line-args": "^5.0.2",
    "fastpriorityqueue": "^0.3.1",
    "grpc": "^1.9.1",
    "gulp": "^3.9.1",
    "http-jsonrpc-server": "^1.0.0-beta.2",
    "mysql2": "^1.5.3",
    "protobufjs": "^6.8.6",
    "run-sequence": "^2.2.1",
    "secp256k1": "^3.5.0",
    "sequelize": "^4.37.3",
    "toml": "^2.3.3",
    "uuid": "^3.2.1",
    "winston": "^3.0.0-rc3",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "gulp-runner": "^1.1.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.2",
    "require-dir": "^1.0.0",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=7.6.0"
  }
